    <declare-styleable name="RulerView">
        <attr name="rv_defaultValue" format="float|reference" />              <!-- 默认值 -->
        <attr name="rv_minValue" format="float|reference" />                  <!-- 最小值 -->
        <attr name="rv_maxValue" format="float|reference" />                  <!-- 最大值 -->
        <attr name="rv_spanValue" format="float|reference" />                 <!-- 精度，最小支持 0.1 -->
        <attr name="rv_itemSpacing" format="dimension|reference" />           <!-- 每个刻度间的宽度 -->
        <attr name="rv_minLineHeight" format="dimension|reference" />         <!-- 最短刻度线长度 -->
        <attr name="rv_maxLineHeight" format="dimension|reference" />         <!-- 最长刻度线长度 -->
        <attr name="rv_middleLineHeight" format="dimension|reference" />      <!-- 中间刻度线长度 -->
        <attr name="rv_minLineWidth" format="dimension|reference" />          <!-- 最短刻度线宽度 -->
        <attr name="rv_maxLineWidth" format="dimension|reference" />          <!-- 最长刻度线宽度 -->
        <attr name="rv_middleLineWidth" format="dimension|reference" />       <!-- 中间刻度线宽度 -->
        <attr name="rv_scaleTextColor" format="color|reference" />            <!-- 刻度盘文字颜色 -->
        <attr name="rv_minLineColor" format="color|reference" />              <!-- 最短刻度线颜色 -->
        <attr name="rv_maxLineColor" format="color|reference" />              <!-- 最大刻度线颜色 -->
        <attr name="rv_middleLineColor" format="color|reference" />           <!-- 中间刻度线颜色 -->
        <attr name="rv_scaleTextSize" format="dimension|reference" />         <!-- 刻度盘文字大小 -->
        <attr name="rv_textMarginTop" format="dimension|reference" />         <!-- 刻度盘文字距离刻度边缘距离 -->
        <attr name="rv_indcatorColor" format="color|reference" />             <!-- 指示器颜色 -->
        <attr name="rv_indcatorWidth" format="dimension|reference" />         <!-- 指示器宽度，形状为三角时不起作用 -->
        <attr name="rv_indcatorHeight" format="dimension|reference" />        <!-- 指示器高度，形状为三角时不起作用 -->
        <attr name="rv_indcatorType" format="enum|reference">                 <!-- 指示器形状 -->
            <enum name="line" value="1" />                                    <!-- 线 -->
            <enum name="triangle" value="2" />                                <!-- 三角 -->
        </attr>
        <attr name="rv_strokeCap" format="enum|reference">                    <!-- 刻度线线帽 -->
            <enum name="round" value="1" />                                   <!-- 圆角 -->
            <enum name="butt" value="0" />                                    <!-- 无圆角 -->
            <enum name="square" value="2" />                                  <!-- 矩形 -->
        </attr>
        <attr name="rv_resultTextColor" format="color|reference" />           <!-- 结果文字颜色 -->
        <attr name="rv_unitTextColor" format="color|reference" />             <!-- 单位文字颜色 -->
        <attr name="rv_resultTextSize" format="dimension|reference" />        <!-- 结果文字大小 -->
        <attr name="rv_unitTextSize" format="dimension|reference" />          <!-- 单位文字大小 -->
        <attr name="rv_unit" format="string|reference" />                     <!-- 单位 -->
        <attr name="rv_showResult" format="boolean|reference" />              <!-- 是否显示结果 -->
        <attr name="rv_showUnit" format="boolean|reference" />                <!-- 是否显示单位 -->
        <attr name="rv_alphaEnable" format="boolean|reference" />             <!-- 是否刻度渐变 -->
    </declare-styleable>

